package group13Project;

import java.io.File;
import java.io.IOException;

public class app {
	public static void main(String[] args) throws IOException{
		double timeS = System.nanoTime();
		//check if Internet connection is valid
		Internet internetCon = new Internet();
		if(internetCon.hasValidInternetConnection()) {
		
			
			Config conf = new Config();
			if(conf.hasConfig() == false) {
				
				File config = conf.createConfig();
				if(config != null) {
					
					//set up new node
					Node node = new Node(config);
					
					
				}
			} else {
				
			}
			System.out.println(System.nanoTime() - timeS );
		} else {
			// No Internet connection
		}
	}
}
