package group13Project;

import group13Project.GUI.*;
import group13Project.security.*;
import java.io.File;
import java.io.IOException;



public class app {
	public static void main(String[] args) throws IOException{
		
		GUIMainFrame frame = new GUIMainFrame(true);
		
		Internet internetCon = new Internet();
		frame.updateStatus("Checking Internet Connection");
		if(internetCon.hasValidInternetConnection()){
			Config conf = new Config();
			System.out.println(conf.hasConfig());
			if(conf.hasConfig() == false) {
				
				File config = conf.createConfig();
				if(config != null) {
					
					//set up new node
					Node node = new Node(config);
					
					
				}
			} else {
				
			}
		}
	}
}
