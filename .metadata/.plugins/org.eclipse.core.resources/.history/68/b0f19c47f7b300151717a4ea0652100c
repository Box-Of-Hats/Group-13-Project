package group13Project;
import java.io.File;
public class Config {
	
	private boolean hasConfig = false;
	private String os;
	
	public Config() {
		//check if config file exist on local system
		setOs(System.getProperty("os.name"));
		System.out(System.getProperty("user.home"));
		switch (this.os){
		case 'Mac OS x' :
			File file = new File('')
			break;
			
		}
		
	}
	private void setOs(String osName) {
		this.os = osName;
	}
	public boolean hasConfig(){
		return hasConfig;
	}
}
